{
  "compilerOptions": {
    "lib": [ "esnext" ],
    "module": "esnext",
    "target": "esnext",
    "types": [ "@cloudflare/workers-types/experimental" ],
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "moduleDetection": "force",
    "verbatimModuleSyntax": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "outDir": "./dist",
    "declaration": true,
    "emitDeclarationOnly": true
  }
}
